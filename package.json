{
  "name": "ollama-ui",
  "version": "1.0.0",
  "description": "Ollama Electron GUI",
  "main": "electron/main.js",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "electron": "electron .",
    "electron:dev": "concurrently \"npm run dev\" \"wait-on http://localhost:5173 && cross-env VITE_DEV_SERVER_URL=http://localhost:5173 electron .\"",
    "electron:build": "npm run build && electron-builder"
  },
  "keywords": [
    "ollama",
    "electron",
    "vue",
    "ai"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@vitejs/plugin-vue": "^6.0.2",
    "concurrently": "^9.2.1",
    "cross-env": "^7.0.3",
    "electron": "^39.2.4",
    "electron-builder": "^26.0.12",
    "vite": "^7.2.6",
    "wait-on": "^8.0.5"
  },
  "build": {
    "appId": "com.ollama.ui",
    "productName": "Ollama UI",
    "files": [
      "dist/**/*",
      "electron/**/*"
    ],
    "directories": {
      "output": "release"
    },
    "win": {
      "target": "nsis",
      "verifyUpdateCodeSignature": false,
      "signAndEditExecutable": false
    },
    "forceCodeSigning": false
  },
  "dependencies": {
    "@element-plus/icons-vue": "^2.3.2",
    "element-plus": "^2.11.9",
    "highlight.js": "^11.11.1",
    "lucide-vue-next": "^0.555.0",
    "markdown-it": "^14.1.0",
    "nanoid": "^5.1.6",
    "pinia": "^3.0.4",
    "vue": "^3.5.25",
    "vue-i18n": "^11.2.2",
    "vue-router": "^4.6.3"
  }
}